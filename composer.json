{
	"name": "gandung/shopee-php-sdk-bundle",
	"type": "symfony-bundle",
	"description": "Symfony bundle module for Shopee PHP SDK.",
	"keywords": [
		"shopee",
		"shopee-api",
		"shopee-sdk",
		"shopee-sdk-api",
		"shopee-api-sdk",
		"symfony-bundle",
		"shopee-sdk-php-bundle",
		"bundle"
	],
	"license": "BSD-3-Clause",
	"config": {
		"sort-packages": true
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	},
	"autoload": {
		"psr-4": {
			"Gandung\\Shopee\\ShopeeBundle\\": ""
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Gandung\\Shopee\\ShopeeBundle\\Tests\\": "Tests/"
		}
	},
	"require": {
		"php": "^7.1",
		"gandung/shopee-php-sdk": "^1.0",
		"symfony/config": "^5.2",
		"symfony/dependency-injection": "^5.2",
		"symfony/http-kernel": "^5.2"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5"
	}
}
